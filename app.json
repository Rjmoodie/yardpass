{
  "expo": {
    "name": "YardPass",
    "slug": "yardpass",
    "version": "1.0.0",
    "sdkVersion": "49.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#6366F1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yardpass.app"
    },
    "android": {
      "package": "com.yardpass.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-camera",
      "expo-location",
      "expo-notifications",
      "expo-image-picker"
    ]
  }
}
